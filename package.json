{
  "name": "isometric-engine",
  "version": "0.1.0",
  "description": "A lightweight 2.5D isometric view engine based on CSS 3D Transform",
  "type": "module",
  "main": "./dist/isometric-engine.umd.cjs",
  "module": "./dist/isometric-engine.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/isometric-engine.js",
      "require": "./dist/isometric-engine.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --port=5373",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "isometric",
    "2.5d",
    "css3d",
    "engine",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-typescript": "^7.28.5",
    "typescript": "^5.3.3",
    "vite": "^7.2.7",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-dts": "^4.3.0"
  },
  "dependencies": {
    "lit": "^3.3.1"
  }
}
